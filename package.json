{
  "author": "PingCAP",
  "name": "key-visualizer",
  "version": "0.0.1",
  "description": "Web interface for pd cluster",
  "scripts": {
    "start": "yarn dev",
    "dev": "parcel src/index.html --hmr-port 18080",
    "build": "parcel build src/index.html --out-dir ./public --public-url ./"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@types/marked": "^0.6",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "^9.7.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "modules": "^0.4.0",
    "parcel-bundler": "^1.12.2",
    "postcss-modules": "^1.4.1",
    "sass": "^1.17.3",
    "typescript": "^3.6.4",
    "typescript-plugin-css-modules": "^1.0.5"
  },
  "dependencies": {
    "@types/lodash": "^4.14.144",
    "d3": "^5.12.0",
    "lodash": "^4.17.15",
    "marked": "^0.6.2",
    "query-string": "^6.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0"
  },
  "postcss": {
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "grid": true
      },
      "postcss-modules": {
        "camelCase": false
      }
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false
  }
}
